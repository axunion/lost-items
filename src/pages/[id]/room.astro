---
import ItemList from "~/components/features/ItemList";
import AppHeader from "~/components/ui/AppHeader.astro";
import Layout from "../../layouts/Layout.astro";

const { id = "" } = Astro.params;
---

<Layout title="Room Items">
  <AppHeader title="Room Items" />

  <main class="px-4 py-6">
    <ItemList listId={id} client:load />
  </main>
</Layout>
